{
  "name": "xcx-jibungoto",
  "version": "0.0.0",
  "scripts": {
    "register": "npx xcratch-register --id=jibungotoPlanet --gui=../scratch-gui --block=./src/vm/extensions/block --entry=./src/gui/lib/libraries/extensions/entry --core --link --use=extension-support util",
    "build": "npx xcratch-build --module=jibungotoPlanet --gui=../scratch-gui --block=./src/vm/extensions/block --entry=./src/gui/lib/libraries/extensions/entry",
    "watch": "tsc -w",
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.ts' 'src/**/index.{js,jsx}'",
    "lint:fix": "eslint --fix 'src/**/*.ts' 'src/**/index.{js,jsx}'",
    "format": "prettier 'src/**/*.ts' 'src/**/index.{js,jsx}'",
    "export:footprints": "ts-node ./data/footprint2ts.ts",
    "export:parameters": "ts-node ./data/parameter2ts.ts",
    "export:all": "npm run export:footprints && npm run export:parameters",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "csv-parse": "^5.3.4",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "xcratch-build": "^0.4.0",
    "xcratch-register": "^0.4.2"
  }
}
